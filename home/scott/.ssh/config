Host arm
     Hostname 10.0.0.236
     User scott
     IdentityFile ~/.ssh/arm
     PreferredAuthentications publickey

Host github.com
     User git
     Hostname github.com
     PreferredAuthentications publickey
     IdentityFile /home/scott/.ssh/github

# EC2 instances via Session Manager
Host i-* mi-*
    User ubuntu
    IdentityFile ~/.ssh/aws
    IdentitiesOnly yes
    ProxyCommand ~/.ssh/ssm-ssh-proxy.sh %h %p